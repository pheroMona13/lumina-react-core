    "outDir": "dist/types",
    "jsx": "react-jsx",
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "importHelpers": true
  },
  "include": ["src"]
}
